{
    "compilerOptions": {
      "target": "es5",
      "lib": [
        "esnext"
      ],
      "skipLibCheck": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "module": "commonjs",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "declaration": true,
    },
    "include": [
      "electron-main", "electron.ts", "common"
    ]
  }
  